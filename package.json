{
  "name": "ecommerce-project",
  "version": "1.0.0",
  "description": "E-commerce project with client and server",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cd client/ecommerce && npm run dev",
    "dev:server": "cd server && npm run dev",
    "start": "cd server && node index.js",
    "build": "cd client/ecommerce && npm run build",
    "install:all": "npm install && cd client/ecommerce && npm install && cd ../.. && cd server && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {}
}